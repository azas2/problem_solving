class Solution {
    public int subtractProductAndSum(int n) {
        int test_sum=0;
        int test_mup=1;
        while(n>0){
            int te=n%10;
            test_sum+=te;
            test_mup*=te;
            n=n/10;
        }
        int solve=test_mup-test_sum;
        return solve;
    }
}
